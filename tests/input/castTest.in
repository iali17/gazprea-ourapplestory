procedure main() returns integer {
      boolean s = true;
      integer u = -58;
      character v = 'a';
      real w = -97.0;

      boolean e = as<boolean>(v);
      boolean f = as<boolean>(u);
      boolean g = as<boolean>(s);

      character x = as<character>(u);
      character y = as<character>(s);
      character z = as<character>(v);

      integer a = as<integer>(v);
      integer b = as<integer>(w);
      integer c = as<integer>(s);
      integer d = as<integer>(u);

      real h = as<real>(s);
      real i = as<real>(u);
      real j = as<real>(v);
      real k = as<real>(w);

      var out = std_output();

      // Print default types
      s -> out;
      u -> out;
      v -> out;
      w -> out;

      '\n' -> out;

      // Print cast to boolean
      e -> out;
      f -> out;
      g -> out;

      '\n' -> out;

      // Print cast to character
      x -> out;
      y -> out;
      z -> out;

      '\n' -> out;

      // Print cast to integer
      a -> out;
      b -> out;
      c -> out;
      d -> out;

      '\n' -> out;

      // Print cast to real
      h -> out;
      i -> out;
      j -> out;
      k -> out;

      return 0;
}
