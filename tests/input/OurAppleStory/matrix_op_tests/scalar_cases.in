procedure main() returns integer {
    var out = std_output();
	var x = [[1,0], [0, 1]];
	var y = [1, 0];
	var z = 1;

	//matrix & vector
	//x || y -> out; //not allowed
	//x ** y -> out; //not allowed
	
	//matrix & scalar
	x || z -> out; '\n' -> out; //allowed. Results in: [[1,0,1,1], [0,1,1,1]];
	x ** z -> out; '\n' -> out; //allowed. Results in multiplication of [[1,0], [0,1]] and [[1,1], [1,1]]
	
	//vector & scalar
	y || z -> out; '\n' -> out; //allowed. Results in: [1,0,1,1]
	y ** z -> out; '\n' -> out;//allowed. Results in dot product of [1,0] and [1,1]
	
	return 0;   
}
