procedure main() returns integer {
var out = std_output();

integer interval t0 = 1 + 1..3;

var t1 = 1..2 + 3..4;
integer interval t2 = (-2)..-1 + (-4)..-3;
var t3 = (-1)..2 + (-4)..3;

t0 by 1 -> out; '\n' -> out;
t1 by 1 -> out; '\n' -> out;
t2 by 1 -> out; '\n' -> out;
t3 by 1 -> out; '\n' -> out;

