procedure main() returns integer {
	var out = std_output();
	var x = 1..10 by 1;
	var y = [i in 1..3, j in 1..3 | i];
	x[*] = -x;
	x[*] -> out; '\n' -> out;
	y[1, *] = [3,3,3];
	y -> out; '\n' -> out;
	y[*, 1] = [2,2,2];
	y[*] -> out; '\n' -> out;
	y[*] = identity;
	y -> out; '\n' -> out;
	return 0;
}
