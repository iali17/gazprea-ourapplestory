procedure main() returns integer {
    integer x = 0;
    integer y = 0;
    var out = std_output();

    loop while y < 3 {
          y = y + 1;

          loop while x >= 0{
            if x >= 3 break;

            x = x + 1;
            '*' -> out;
          }
          x = 0;

          '\n' -> out;
    }

    return 0;
}