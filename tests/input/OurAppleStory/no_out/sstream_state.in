procedure main() returns integer { 
	var inp = std_input();
	var out = std_output();
	character x;
	integer y;
	integer xss;
	integer yss;
	x<- inp;
	xss = stream_state(inp);
	y<- inp;
	yss = stream_state(inp);
	
	'\n'-> out;
	xss->out; '\n'-> out;
	yss->out; '\n'-> out;
	'\n'-> out;
	x->out; '\n'-> out;
	y->out; '\n'-> out;
  return 0; 
}
