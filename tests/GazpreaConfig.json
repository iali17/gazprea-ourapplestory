{
  "inDir": "input/OurAppleStory",
  "outDir": "output/OurAppleStory",
  "testedExecutablePaths": {
    "<gazprea-ourapplestory>": "../bin/gazc"
  },
  "runtimes": {
    "<gazprea-ourapplestory>": "../bin/libgazrt.so"
  },
  "toolchains": {
    "gazprea": [
      {
        "stepName": "gazc",
        "executablePath": "$EXE",
        "arguments": [
          "$INPUT",
          "$OUTPUT"
          ],
        "output": "gazc.ll"
      },
      {
          "stepName": "lli",
	  
        "executablePath": "~/llvm-ins/bin/lli",
          "arguments": [
          "$INPUT"
        ],
        "output": "-",
        "usesRuntime": true
      }
    ]
  }
}
