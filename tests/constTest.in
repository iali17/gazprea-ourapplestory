procedure main() returns integer {
	var out = std_output();

	const integer x = 5;
	const real y = 10.0;
	const boolean z = true;
	const character c = 'a';

	// Print default variables
	x -> out;
	'\t' -> out;
	y -> out;
    '\t' -> out;
	z -> out;
    '\t' -> out;
	c -> out;
    '\n' -> out;

	// Test assignment to const
	x = 10;
	x -> out;

    '\t' -> out;

	y = 1.0;
	y -> out;

    '\t' -> out;

	z = false;
	z -> out;

    '\t' -> out;

	c = 'b';
	c -> out;

    '\n' -> out;

	return 0;
}
